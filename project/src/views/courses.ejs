<script>
  function check(item){
    var cartNum = document.getElementById("cartNum");
    let arr = localStorage.getItem('cart_courses');
    if (arr == null) {
      arr = [];
    } else {
      arr = JSON.parse(localStorage.getItem('cart_courses'));
    }
    if (arr.includes(item)) return;
    arr.push(item);

    cartNum.innerHTML = arr.length;
    localStorage.setItem('cart_courses', JSON.stringify(arr));
  }
</script>

<script>

  var shopCart = document.getElementById("shopCart");
  shopCart.addEventListener("click",()=>{
    var user = JSON.parse(localStorage.getItem("loginValid"));
    if(user.signed){
      window.location.href = "/interntheory/courses/cart";
    }else{
      window.open("/interntheory/login");
    }

  })
</script>